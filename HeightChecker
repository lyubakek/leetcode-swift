// Question link: https://leetcode.com/problems/height-checker/

class Solution {
    func heightChecker(_ heights: [Int]) -> Int {
        var sortedHeights = heights
        sortedHeights.sort()
        var diff = 0
        for i in 0...heights.count - 1 {
            if heights[i] != sortedHeights[i] {
                diff += 1
            }
        }
        return diff
    }
}
